<%= form_for(@picture, multipart: true, url: create_photos_quad_buildings_path, method:"put") do |f| %>

  <div class="form-group comment-tab">
  </div>
  <div class="field">
    <%= f.file_field :image %>
  </div>
  <div class="actions">
    <%= hidden_field_tag "quad_id", params[:id] %>
    <%= hidden_field_tag "building_id", @building.id %>
    <%= f.submit "Upload", class: 'btn btn-success'%>
  </div>
<% end %>
