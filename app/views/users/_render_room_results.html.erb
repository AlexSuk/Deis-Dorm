<div class = "row">
	<br></br>
    <h2> Your Ideal Room Matches: </h2>
</div>
<br>
<div class = "row">
	<table class = "table table-bordered table-striped">
		<thead>
		    <tr>
		      <th> Room# </th>
		      <th> Building </th>
		    </tr>
		</thead>

		<tbody>
		    <% @rooms.each do |room| %>
		      <tr>
		        <td><%= room.number %></td>
		        <td><%= @buildings[@rooms.index(room)].name %></td>
		      </tr>
		    <% end %>
		</tbody>
	</table>
</div>