<p>
  <strong>Commenter:</strong>
  <%user = User.find_by(id: post.user_id)%>
  <%= image_tag user.icon.url(:thumb) %>
  <%= user.user_name %>
  <%# image_tag @users[post.user_id][:icon] %>
  <%# @users[post.user_id][:name] %>
</p>
<p>
  <strong>Comment:</strong>
  <br>
  <% post.pictures.each do |pic| %>
    <%= image_tag pic.image.url(:square) %>
  <% end %>
  <br>
  <%= post.text %>
</p>
